.top {
    height: 350rpx;
    width: 100%;
    position: relative;
}

.top .bj {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: -1;
    bottom: 0;
}

.user {
    text-align: center;
    padding-top: 72rpx;
}

.user .user_img {
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
}

.user text {
    color: #fff;
}

.income {
    margin: -36rpx 30rpx 0;
    border: 1px solid #eee;
    box-shadow: 0 7px 10px 0 rgba(0,0,0,.05);
    border-radius: 10px;
    background: #fff;
}

page {
    height: 100%;
    background: #f5f5f5;
}

.income .btns {
    font-size: 24rpx;
    color: #c28e90;
    padding: 18rpx 43rpx;
    border: 1px solid #c28e90;
    border-radius: 5px;
    width: 100rpx;
    margin: 60rpx auto 0;
}

.btns_list {
    margin-top: 56rpx;
    display: flex;
    justify-content: space-between;
}

.btns_list view {
    text-align: center;
}

.btns_list .f24 {
    color: #999;
    margin-bottom: 20rpx;
}

.btns_list .f30 {
    padding-bottom: 40rpx;
}

.user_list {
    margin: 30rpx 30rpx 60rpx;
    background: #fff;
    border: 1px solid #eee;
    box-shadow: 0 7px 10px 0 rgba(0,0,0,.05);
    border-radius: 10px;
}

.user_top {
    display: flex;
    align-items: center;
    padding-top: 30rpx;
}

.user_top image {
    width: 23rpx;
    height: 27rpx;
}

.user_top>view {
    flex: 1;
    text-align: center;
    font-size: 24rpx;
}

.kong {
    margin-top: 80rpx;
    padding-bottom: 60rpx;
}

.kong image {
    width: 155rpx;
    height: 200rpx;
}

.kong .f24 {
    color: #999;
}

.kong .btns {
    width: 426rpx;
    height: 70rpx;
    line-height: 70rpx;
    color: #fff;
    text-align: center;
    background: #3d365e;
    border-radius: 10px;
    margin: 88rpx auto 0;
}

.usernamelist {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 20rpx;
}

.usernamelist image {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
}

.usernamelist>view:first-child {
    display: flex;
    align-items: center;
}

.wrapper {
    text-align: center;
    margin-top: 40rpx;
}

.wrapper image {
    width: 690rpx;
    height: 1075rpx;
}