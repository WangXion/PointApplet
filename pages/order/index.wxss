.tabs .van-tabs__scroll--line {
    background: #f5f5f5;
}

.tabs .van-tab--active {
    color: #c28e90;
}

page {
    height: 100%;
    background: #f5f5f5;
}

.order {
    margin: 30rpx;
    background: #fff;
    border-radius: 5px;
    padding: 30rpx;
}

.order .title {
    padding-bottom: 32rpx;
    border-bottom: 1px solid #eee;
    justify-content: space-between;
}

.conter,.order .title {
    display: flex;
}

.conter {
    padding: 45rpx 0;
}

.conter .bottom {
    position: absolute;
    bottom: 0;
    display: flex;
    width: 100%;
    justify-content: space-between;
}

.bottom text {
    color: #999;
}

.conter .aaa {
    width: 200rpx;
    height: 200rpx;
    margin-right: 44rpx;
}

.conter .aaa image {
    width: 100%;
    height: 100%;
}

.conter .f26 {
    margin-top: 16rpx;
    color: #999;
}

.amout {
    display: flex;
    font-size: 28rpx;
    align-items: center;
    padding-bottom: 60rpx;
    border-bottom: 1px solid #eee;
}

.amout view {
    margin-left: 32rpx;
}

.amout text {
    color: #c28e90;
}

.btns {
    display: flex;
    align-items: center;
    justify-content: flex-end;
    margin-top: 30rpx;
}

.btns view {
    font-size: 28rpx;
    padding: 22rpx 25rpx;
    border: 1px solid #eee;
    margin-left: 20rpx;
}

.useBg {
    background-color: #f5f5f5;
    border-radius: 5rpx;
    padding: 5rpx 0;
}

.btns .pay {
    background: #c28e90;
    color: #fff;
    border-radius: 8rpx;
}

.btns .tix {
    color: #c28e90;
}

.btns .payOne,.btns .tix {
    border-radius: 8rpx;
}