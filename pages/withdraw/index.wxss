.top {
    height: 350rpx;
    width: 100%;
    position: relative;
}

.top .bj {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: -1;
    bottom: 0;
}

.user {
    text-align: center;
    padding-top: 72rpx;
}

.user .user_img {
    width: 112rpx;
    height: 112rpx;
    border-radius: 50%;
}

.user text {
    color: #fff;
}

.amout {
    background: #fff;
    border: 1px solid #eee;
    box-shadow: 0 7px 10px 0 rgba(0,0,0,.05);
    border-radius: 10px;
    margin: -30rpx 30rpx 40rpx;
    padding-bottom: 40rpx;
}

.money {
    position: relative;
    overflow: hidden;
    display: flex;
    margin: 30rpx 0;
    border-bottom: 1rpx solid #dcdada;
}

.amout .f26 {
    padding-top: 33rpx;
}

.inputs {
    font-size: 30rpx;
}

.amout .van-cell__title {
    font-size: 40rpx;
}

.tx {
    margin: 0 30rpx;
    background: #fff;
    border: 1px solid #eee;
    box-shadow: 0 7px 10px 0 rgba(0,0,0,.05);
    border-radius: 10px;
}

.blak_list {
    display: flex;
    align-items: center;
    padding: 40rpx;
    border-bottom: 1px solid #eee;
    justify-content: space-between;
}

.blak_list .icon {
    width: 67rpx;
    height: 70rpx;
}

.blak_list view {
    font-size: 26rpx;
}

.blak_list view text {
    margin-left: 30rpx;
}

.blak_list .rr {
    width: 10rpx;
    height: 18rpx;
}

.btns {
    margin: 100rpx auto 0;
}

.tx .bg {
    background: #f5f5f5;
    color: #000;
}

.edit {
    height: 50rpx;
    width: 20px;
    display: flex;
    align-items: center;
    justify-content: center;
}

page {
    height: 100%;
    background: #f5f5f5;
}

.btns {
    width: 462rpx;
    height: 70rpx;
    background: #3d365e;
    border-radius: 10px;
    line-height: 70rpx;
    margin: 0 auto;
    text-align: center;
    color: #fff;
    font-size: 32rpx;
}

.bgBack {
    opacity: .4;
}