    font-weight: 500;
    color: #333;
}

.express .right {
    background-color: rgba(8,175,254,.02);
    border-radius: 0rpx 0rpx 20rpx 20rpx;
    padding: 15rpx 30rpx 20rpx;
}

.express .right .text {
    color: #666;
    font-size: 22rpx;
}

.Name {
    width: 100%;
    display: flex;
    justify-content: left;
    flex-wrap: wrap;
    align-items: center;
}

.Name .dizhi {
    width: 26rpx;
    height: 32rpx;
    margin-right: 17rpx;
}

.content {
    padding: 56rpx 46rpx 56rpx 5rpx;
    border-radius: 20rpx;
    box-shadow: 0 7rpx 10rpx 0 rgba(0,0,0,.05);
}

.content .list:first-child .info:before {
    bottom: -20rpx;
    margin-top: 40rpx;
    border-left: 1px dashed #e5e5e5;
}

.content .list:first-child .title {
    margin-bottom: 20rpx;
}

.content .list:first-child .title:before {
    width: 46rpx;
    height: 46rpx;
    left: -23rpx;
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 46rpx 46rpx;
}

.content .list:last-child .info:before {
    height: 32rpx;
}

.content .list.one .info:before {
    margin-top: 20rpx;
}

.content .list.one .title {
    color: #1a1a1a;
}

.content .list.one .title:before {
    width: 46rpx;
    height: 46rpx;
    left: -23rpx;
    background: url("data:image/png;base64,") no-repeat 50%;
    background-size: 46rpx 46rpx;
}

.content .list.one .text {
    color: #666;
}

.content .list.one .time {
    color: #333;
}

.content .list.one .time .day {
    font-size: 24rpx;
}

.content .timeOver {
    font-size: 20rpx;
    padding-left: 32rpx;
    font-size: 23rpx;
    color: #333;
}

.content .time,.content .timeOver {
    font-family: PingFang SC;
    font-weight: 500;
}

.content .time {
    padding-top: 13rpx;
    font-size: 20rpx;
    color: #999;
    display: flex;
    flex-direction: column;
}

.content .time .day,.content .time .go {
    margin-bottom: 4rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: #999;
}

.content .time .day {
    padding-left: 32rpx;
    font-size: 20rpx;
}

.content .time .ti {
    margin-bottom: 4rpx;
    padding-left: 32rpx;
}

.content .info,.content .time .ti {
    font-size: 20rpx;
    font-family: PingFang SC;
    font-weight: 500;
    color: #999;
}

.content .info {
    display: flex;
    position: relative;
    padding-top: 12rpx;
    margin-left: 35rpx;
}

.content .info:before {
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    bottom: 0;
    z-index: 1;
    width: 0;
    border-left: 1px solid #e5e5e5;
}

.content .info .title {
    position: relative;
    margin-bottom: 23rpx;
    padding-left: 32rpx;
    font-size: 28rpx;
}

.content .info .title:before {
    content: "";
    position: absolute;
    left: -3px;
    top: 0;
    bottom: 0;
    z-index: 1;
    width: 7px;
    height: 7px;
    margin: auto 0;
    border-radius: 50%;
    background-color: #cecece;
}

.content .info .title.address {
    font-size: 24rpx;
    color: #333;
}

.content .info .text {
    padding: 0 0 44rpx 32rpx;
}

.loca {
    width: 100%;
    margin-left: 15rpx;
    border-bottom: 1px solid #f5f5f5;
    padding-bottom: 33rpx;
}

.loca,.loca .left {
    display: flex;
    height: 50rpx;
    align-items: center;
}

.loca .left {
    width: 25%;
    font-size: 28rpx;
}

.loca .right {
    height: 50rpx;
    width: 75%;
    background: #eee;
    border: 1px solid #eee;
    border-radius: 5px;
}

.loca .right .inp {
    position: relative;
}

.loca .right .inp image {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    right: 23rpx;
    top: -1rpx;
}

.loca .right input {
    width: 100%;
    height: 50rpx;
    align-items: center;
    padding-left: 20rpx;
    font-size: 25rpx;
}