    height: 88rpx;
    border-radius: 50%;
}

.express .top .text {
    margin-left: 20rpx;
}

.express .top .text .text1 {
    font-size: 26rpx;
    font-family: PingFang SC;
    font-weight: 700;
    margin-bottom: 15rpx;
