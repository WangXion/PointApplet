.page {
    height: 100vh;
    height: calc(100vh - 44px);
    font-size: 24rpx;
    background-color: #f6f7f8;
    overflow-y: auto;
}

.flex {
    display: flex;
    flex-direction: row;
    flex-direction: column;
    margin-bottom: 20rpx;
}

.flex1 {
    flex: 1;
}

.ali-c {
    display: flex;
    flex-direction: row;
    align-items: center;
}

.bgf,.express {
